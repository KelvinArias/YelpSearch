{
  "name": "yelp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "server": "nodemon server.js",
    "client": "next dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "classnames": "^2.3.2",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^5.0.0",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "yelp-fusion": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.1",
    "sass": "^1.69.5"
  }
